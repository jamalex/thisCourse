<script id="item_probe" type="text/x-handlebars-template">
 <div class='probe container'>
    <h3 class='question'>{{question.text}}</h3>
    <ol>
    {{#each answers}}
        <li>{{this.text}}</li>
    {{/each}}
    </ol>

    <button class='btn btn-large btn-success'>Submit Answer</button>
</div>   

</script>


<script id="item_probe_response" type="text/x-handlebars-template">
<div class='response container'>
    <div class='header'>
        <h3>{{response.response}}</h3>
        {{#if response.feedback}}<button class='btn btn-large btn-primary' id='feedbut'>View Feedback</button>{{/if}}
        <button class='btn btn-large btn-success'>Next Question</button>
    </div>
    {{#if response.feedback}}<div id='feedback'>
        {{{response.feedback}}}
    </div>
    {{/if}}
</div>


<script id="item_probe_edit" type="text/x-handlebars-template">
    

</script>